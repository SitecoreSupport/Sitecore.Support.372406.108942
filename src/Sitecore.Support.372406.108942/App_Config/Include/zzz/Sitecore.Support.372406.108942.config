<?xml version="1.0" encoding="utf-8" ?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
    <sitecore>
        <events>
            <event name="media:request">
                <!-- Sitecore.Analytics.Tracking.config -->
                <handler type="Sitecore.Analytics.Media.MediaRequestEventHandler, Sitecore.Analytics" method="OnMediaRequest">
                    <patch:delete />
                </handler>
                <!-- Sitecore.Analytics.Tracking.RobotDetection.config -->
                <handler type="Sitecore.Analytics.RobotDetection.Media.MediaRequestEventHandler, Sitecore.Analytics.RobotDetection">
                    <patch:delete />
                </handler>
                <!-- Sitecore.Support.108942.config -->
                <handler type="Sitecore.Support.Analytics.Media.MediaRequestEventHandler, Sitecore.Support.108942">
                    <patch:delete />
                </handler>
                <!-- Sitecore.Support.108942.config -->
                <handler type="Sitecore.Support.Analytics.RobotDetection.Media.MediaRequestEventHandler, Sitecore.Support.372406">
                    <patch:delete />
                </handler>

                <handler type="Sitecore.Support.Analytics.RobotDetection.Media.MediaRequestEventHandler, Sitecore.Support.108942.372406" method="OnMediaRequest"/>
            </event>
        </events>
    </sitecore>
</configuration>
